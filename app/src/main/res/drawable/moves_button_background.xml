<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:angle="0"
                android:endColor="#D27D2D"
                android:startColor="#9C5333" />
            <corners android:radius="10dp" />
            <stroke android:width="2dp" android:color="?android:attr/textColorPrimary" />
            <padding android:left="15dp" android:bottom="10dp" />
        </shape>
    </item>
</layer-list>